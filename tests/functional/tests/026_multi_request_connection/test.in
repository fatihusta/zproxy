DESCRIPTION="Send two GETs in the same connection, the first with body"
CMD=curl
SSL=1
CL=1
METHOD="GET"
BODY="file.txt"
URL="/echo"
HEADERS="Content-Type: text/xml"
NEXT_METHOD="GET"
NEXT_URL="/"
