DESCRIPTION="Add a session to the service"
CMD=ctl
CL=1
METHOD="PUT"
URL="/listener/0/services/0/sessions"
BODY=static_session.cfg
#########################
DESCRIPTION="wait for checking the session does not disappear"
CMD=wait
TIMEOUT=10
#########################
DESCRIPTION="Get stats"
CMD=ctl
URL="/listener/0"
METHOD="GET"
#########################
DESCRIPTION="do requests"
CMD=average
CL=1
METHOD="GET"
URL="/"
REQUESTS=4
#########################
DESCRIPTION="Get stats"
CMD=ctl
URL="/listener/0"
METHOD="GET"
#########################
DESCRIPTION="Modify static session"
CMD=ctl
CL=1
METHOD="PUT"
URL="/listener/0/services/0/sessions"
BODY=static_session_2.cfg
#########################
DESCRIPTION="Remove the static session"
CMD=ctl
CL=1
METHOD="DELETE"
URL="/listener/0/services/0/sessions/10.1.1.1"
#########################
DESCRIPTION="do requests"
CMD=average
CL=1
METHOD="GET"
URL="/"
REQUESTS=4
#########################
DESCRIPTION="Get stats"
CMD=ctl
URL="/listener/0"
METHOD="GET"
#########################
DESCRIPTION="Configure again the static session"
CMD=ctl
CL=1
METHOD="PUT"
URL="/listener/0/services/0/sessions"
BODY=static_session_2.cfg
